MONGODB_URI=mongodb_url_here

# Clerk keys (from Clerk dashboard)
CLERK_SECRET_KEY=clerk_server_key_here
CLERK_FRONTEND_API=clerk_frontend_api_here
CLERK_JWT_KEY=clerk_jwk_key_here

# Frontend origin(s) - comma separated for dev and prod
FRONTEND_ORIGIN=http://localhost:5173,https://waypoint-demo-two.vercel.app

# AI Provider Configuration
# Set AI_PROVIDER to either "openai" or "gemini"
AI_PROVIDER=

# OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Key (get from https://aistudio.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# AI Model Selection (optional - defaults to gpt-4o for OpenAI, gemini-2.5-flash for Gemini)
AI_MODEL=

# Admin email used for quick admin checks (seed/demo)
ADMIN_EMAIL=admin@example.edu

# AES 32-byte secret for encrypting consented sensitive fields (base64)
ENCRYPTION_KEY=base64_or_hex_32_bytes_here
